{
    "name": "bitwarden-to-1password",
    "version": "0.0.1",
    "description": "Converts Bitwarden exported logins to accepted 1password csv format",
    "main": "build/bw-to-1p.js",
    "author": "Sam Huynh <samhwang2112.dev@gmail.com>",
    "license": "ISC",
    "engines": {
        "node": "^14.0.0",
        "npm": "^8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samhwang/bitwarden-to-1password.git"
    },
    "scripts": {
        "prepare": "husky install",
        "clean:all": "npm run prebuild && npm run pretest",
        "prebuild": "rimraf build",
        "typecheck": "tsc",
        "build:only": "node scripts/build.js",
        "build": "npm run typecheck && npm run build:only",
        "pretest": "rimraf coverage",
        "test": "jest",
        "lint": "eslint src --ext .ts",
        "lint:fix": "npm run eslint -- --fix",
        "format": "npm run lint && prettier -w src/**/*"
    },
    "dependencies": {
        "commander": "^8.3.0"
    },
    "devDependencies": {
        "@jest/types": "^27.1.0",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.11.1",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "esbuild": "^0.13.8",
        "eslint": "^8.0.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^14.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.0",
        "jest": "^27.1.0",
        "lint-staged": "^11.1.2",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.2.0",
        "typescript": "^4.4.4"
    }
}
