{
    "name": "bitwarden-to-1password",
    "version": "0.0.2",
    "description": "Converts BitWarden exported logins to 1Password-compatible csv format",
    "main": "build/main.js",
    "author": "Sam Huynh <samhwang2112.dev@gmail.com>",
    "license": "ISC",
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=7.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samhwang/bitwarden-to-1password.git"
    },
    "scripts": {
        "prepare": "husky install",
        "clean:all": "npm run prebuild && npm run pretest",
        "prebuild": "rimraf build",
        "build": "ts-node scripts/build.ts",
        "pretest": "rimraf coverage",
        "test:only": "jest",
        "typecheck": "tsc",
        "test": "concurrently --kill-others-on-fail npm:typecheck npm:test:only",
        "lint": "eslint . --ext .ts",
        "lint:fix": "npm run eslint -- --fix",
        "prettier": "prettier -w lib/* scripts/* bin/*",
        "format": "npm run lint && npm run prettier"
    },
    "dependencies": {
        "commander": "^8.3.0",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@jest/types": "^27.1.0",
        "@types/concurrently": "^6.4.0",
        "@types/jest": "^27.0.1",
        "@types/node": "^16.11.1",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "concurrently": "^6.4.0",
        "esbuild": "^0.14.2",
        "esbuild-jest": "^0.5.0",
        "eslint": "^8.0.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.0",
        "jest": "^27.4.4",
        "lint-staged": "^12.1.2",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.2.0",
        "typescript": "^4.5.3"
    }
}
